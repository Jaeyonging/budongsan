{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://apis.data.go.kr/$1"
        },
        {
            "source": "/monthapi/(.*)",
            "destination": "http://openapi.molit.go.kr:8081/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}