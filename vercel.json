{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "http://openapi.molit.go.kr/$1"
        },
        {
            "source": "/monthapi/(.*)",
            "destination": "http://openapi.molit.go.kr:8081/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ]
}